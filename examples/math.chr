// Math functions example

func main() -> Int {
    // Integer math
    print(abs(-42));
    print(min(10, 20));
    print(max(10, 20));

    // Float math
    var s = sqrt(16.0);
    print(s);

    var p = pow(2.0, 10.0);
    print(p);

    print(floor(3.7));
    print(ceil(3.2));
    print(round(3.5));

    // Trig
    print(sin(0.0));
    print(cos(0.0));

    return 0;
}
