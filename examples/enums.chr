enum Color {
    Red,
    Green,
    Blue
}

func main() {
    var c = Color.Green;
    match c {
        Color.Red => print("red")
        Color.Green => print("green")
        Color.Blue => print("blue")
    }

    var code = match c {
        Color.Red => 0
        Color.Green => 1
        Color.Blue => 2
    };
    print(code);
}
